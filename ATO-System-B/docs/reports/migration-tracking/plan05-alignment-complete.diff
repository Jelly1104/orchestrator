+MANIFEST_VERSION=$(grep "Version" .claude/SYSTEM_MANIFEST.md | head -1 | sed 's/.*: \([0-9.]*\).*/\1/')
+echo "✅ SYSTEM_MANIFEST 버전: v$MANIFEST_VERSION"
